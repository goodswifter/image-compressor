{
  "name": "compressor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "dev:client": "vite --mode dev",
    "dev:server": "cd server && pnpm run dev",
    "dev:uat": "vite --mode uat",
    "dev:prod": "vite --mode prod",
    "build": "vue-tsc -b && vite build --mode dev",
    "build:uat": "vue-tsc -b && vite build --mode uat",
    "build:prod": "vue-tsc -b && vite build --mode prod",
    "preview": "vite preview",
    "server:install": "cd server && pnpm install",
    "server:start": "cd server && pnpm start",
    "gen:images": "node scripts/generate-assets.js",
    "gen:images:once": "node scripts/generate-assets.js --no-watch",
    "format": "prettier --write .",
    "lint": "eslint src --ignore-pattern assets",
    "lint:fix": "eslint --fix src --ignore-pattern assets",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,css,scss}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@wlydfe/pinia-plus": "^1.0.2",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.10.5",
    "jszip": "^3.10.1",
    "pinia": "^3.0.3",
    "sharp": "^0.34.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/ep": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.2.0",
    "@unocss/eslint-plugin": "^66.4.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/tsconfig": "^0.7.0",
    "@wlydfe/eslint-config": "^0.0.4",
    "@wlydfe/unocss-config": "^1.0.3",
    "boxen": "^8.0.1",
    "chokidar": "^4.0.3",
    "editorconfig": "^3.0.1",
    "eslint": "^9.32.0",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.1",
    "globals": "^16.3.0",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "node-fetch": "^3.3.2",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.90.0",
    "stylelint": "^16.23.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "unocss": "^66.4.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.5"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "22.18.0",
      "onFail": "download"
    }
  }
}
